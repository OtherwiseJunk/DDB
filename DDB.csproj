<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>DartsDiscordBots</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Dart</Authors>
    <Company />
    <Version>2.0.9</Version>
    <Description>Collection of modules and utilties for Discord bots based on Discord.net</Description>
    <AssemblyVersion>2.0.9.0</AssemblyVersion>
    <PackageReleaseNotes>Breaking change to Message Reliability Service which should simplify use.
Added Jackbox Module that provides functionality for voting on games, ranking games, getting game descriptions, and setting custom descriptions, voting emoji, and player descriptions for each guild.</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/OtherwiseJunk/DDB</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <FileVersion>2.0.9.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Discord.Net" Version="2.3.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="5.0.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="5.0.0" />
    <PackageReference Include="NLog" Version="4.7.8" />
    <PackageReference Include="Victoria" Version="5.1.9" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Modules\ServerManagement\Models\" />
  </ItemGroup>

</Project>
